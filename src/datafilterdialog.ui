<connections>
    <connection>
        <sender>filterKCB</sender>
        <signal>activated(int)</signal>
        <receiver>Datafilterdialog</receiver>
        <slot>slotSetFilterText(int)</slot>
    </connection>
</connections>
<slots>
    <slot>slotSetFilterText(int id)</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
<includehints>
    <includehint>kcombobox.h</includehint>
    <includehint>klineedit.h</includehint>
    <includehint>kpushbutton.h</includehint>
