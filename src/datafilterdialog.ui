                <cstring>layout10</cstring>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="KComboBox">
                    <item>
                        <property name="text">
                            <string>Metals</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Gases</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Groups</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>filterKCB</cstring>
                    </property>
                </widget>
                <widget class="KLineEdit">
                    <property name="name">
                        <cstring>groupsKLE</cstring>
                    </property>
                </widget>
            </hbox>
